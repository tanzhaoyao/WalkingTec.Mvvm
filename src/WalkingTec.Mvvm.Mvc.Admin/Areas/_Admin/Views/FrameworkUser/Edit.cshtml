@model WalkingTec.Mvvm.Mvc.Admin.ViewModels.FrameworkUserVms.FrameworkUserVM

<wt:form vm="@Model">
    <wt:tab>
        <wt:tabheaders>
            <wt:tabheader title="基本信息" />
            <wt:tabheader title="附加信息" />
        </wt:tabheaders>
        <wt:tabcontents>
            <wt:tabcontent>
                <wt:row items-per-row="ItemsPerRowEnum.Two">
                    <wt:textbox field="Entity.ITCode" />
                    <wt:combobox field="Entity.IsValid" />
                </wt:row>
                <wt:row items-per-row="ItemsPerRowEnum.Two">
                    <wt:textbox field="Entity.Name" />
                    <wt:upload field="Entity.PhotoId" upload-type="ImageFile" thumb-width="128" />
                </wt:row>
                <wt:checkbox field="SelectedRolesIDs" items="AllRoles" />
                <wt:checkbox field="SelectedGroupIDs" items="AllGroups" />
            </wt:tabcontent>
            <wt:tabcontent>
                <wt:row items-per-row="ItemsPerRowEnum.Two">
                    <wt:combobox field="Entity.Sex" />
                </wt:row>
                <wt:row items-per-row="ItemsPerRowEnum.Two">
                    <wt:textbox field="Entity.CellPhone" />
                    <wt:textbox field="Entity.HomePhone" />
                </wt:row>
                <wt:row items-per-row="ItemsPerRowEnum.Two">
                    <wt:textbox field="Entity.Email" />
                    <wt:textbox field="Entity.ZipCode" />
                </wt:row>
                <wt:textbox field="Entity.Address" />
            </wt:tabcontent>
        </wt:tabcontents>
    </wt:tab>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
